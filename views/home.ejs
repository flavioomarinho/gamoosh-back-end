<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
	integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

<link rel='stylesheet' href='css/home.css'>
<table class="table table-striped">
	<tr class="bg-info">
		<th>Serie</th>
		<th>Mac</th>
		<th>Ip</th>
		<th>Estado</th>
		<th>Calibração</th>
		<th>Erro</th>
		<th>Versão</th>
		<th>Localização</th>
		<th>Detalhes</th>
	</tr>

	<tbody id="myTable">
		<% for(var i = 0; i < arrayObjetos.length; ++ i) { %>

		<tr id="linha">
			<td><%=arrayObjetos[i].serie%></td>
			<td><%=arrayObjetos[i].mac%></td>
			<td><%=arrayObjetos[i].ip%></td>
			<td><%=arrayObjetos[i].estado%></td>
			<td><%=arrayObjetos[i].calibracao%></td>
			<td><%=arrayObjetos[i].erro%></td>
			<td><%=arrayObjetos[i].versao%></td>
			<td><%=arrayObjetos[i].localizacao%></td>
			<td><button id="detalhes">Detalhes</button></td>
		</tr>

		<%}%>
		</tbody>
	
		<script>
			var detalhes = document.getElementById('detalhes');
			detalhes.onclick = function () {
				location.assign('http://localhost:3000/dashboard');
			}

		</script>
		</script>
	</table>